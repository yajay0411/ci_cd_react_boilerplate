{
  "name": "sip_retention",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env vite",
    "build:development": "cross-env vite build --mode development",
    "build:production": "cross-env vite build --mode production",
    "preview": "cross-env vite preview --port 9090",
    "test": "vitest --mode test",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "lint:ts": "eslint ./src --ext .ts,.tsx",
    "lint:ts:fix": "eslint ./src --ext .ts,.tsx --fix",
    "lint:scss": "stylelint \"src/**/*.{scss,css}\"",
    "lint:scss:fix": "stylelint \"src/**/*.{scss,css}\" --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,scss,css}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm-run-all --continue-on-error --parallel lint:scss lint:ts type-check format",
    "validate:fix": "npm-run-all --continue-on-error --parallel lint:scss:fix lint:ts:fix format",
    "prepare": "husky install",
    "clean:build": "npx rimraf build",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md}": [
      "prettier -w",
      "eslint --fix --no-warn-ignored"
    ],
    "*.{css,scss}": [
      "prettier -w",
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@bugsnag/js": "^8.4.0",
    "@bugsnag/plugin-react": "^8.4.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@mui/x-date-pickers": "^8.10.0",
    "axios": "^1.11.0",
    "bootstrap": "^5.3.8",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.6",
    "dotenv": "^17.2.1",
    "firebase": "^12.1.0",
    "formik": "^2.4.6",
    "framer-motion": "^12.23.12",
    "normalize.css": "^8.0.1",
    "notistack": "^3.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-google-recaptcha": "^3.1.0",
    "react-gtm-module": "^2.0.11",
    "react-router-dom": "^7.8.0",
    "swiper": "^11.2.10",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.56.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.7.2",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-gtm-module": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "cross-env": "^10.0.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.3.0",
    "happy-dom": "^20.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "npm-run-all": "^4.1.5",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "sass": "^1.90.0",
    "stylelint": "^16.23.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-scss": "^6.12.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vite-plugin-checker": "^0.10.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "volta": {
    "node": "22.18.0"
  }
}
